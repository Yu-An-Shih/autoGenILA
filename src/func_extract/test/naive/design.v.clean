module adder(a0, b0, c);
  input [1:0] a0;
  input [1:0] b0;
  output [1:0] c;
  wire _0_;
  assign _0_ = a0 > 2'b0;
  assign c = _0_ ? a0 : b0;
endmodule

module word_adder(a, b, result);
  input [1:0] a;
  input [1:0] b;
  output [3:0] result;
  wire [1:0] tmp1;
  wire [1:0] tmp2;
  adder u0 (
    .a0(a),
    .b0(b),
    .c(tmp1)
  );
  adder u1 (
    .a0(a),
    .b0(b),
    .c(tmp2)
  );
  //assign result = tmp1 & tmp2;
  assign result[1:0] = tmp1 & tmp2;
  assign result[3:2] = tmp1 & tmp2;
  assign result = { result[3:2], result[1:0] };
endmodule
